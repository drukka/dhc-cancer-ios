platform :ios, '12.0'

def shared_pods 
  pod 'NVActivityIndicatorView'
  pod 'Kingfisher'
  pod 'Locksmith'
  pod 'PromiseKit/Alamofire'
  pod 'Swinject'
  pod 'EmptyDataSet-Swift', '~> 4.2.0'
  pod 'AlamofireNetworkActivityIndicator', '~> 2.4'
  pod 'SwiftyAttributes'
  pod 'ImageSlideshow/Kingfisher'
  pod 'KeyboardDucker'
  
end

target 'DHCCancer' do
  use_frameworks!
  inhibit_all_warnings!
  shared_pods

end

target 'DHCCancer-mock' do
  use_frameworks!
  inhibit_all_warnings!
  shared_pods

end
